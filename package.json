{
  "name": "mongoose-paginate-cursor",
  "version": "0.9.0",
  "description": "Module for pagination with cursor on mongoose",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidlondono/mongoose-paginate-cursor.git"
  },
  "scripts": {
    "pretest": "npm run lint || true",
    "test": "NODE_ENV=test mocha -u bdd -R spec --require babel-core/register",
    "clean": "rimraf lib && mkdir lib",
    "lint": "eslint src test",
    "lint:fix": "eslint src --fix",
    "build": "npm run clean && npm run build:module",
    "build:module": "babel -d ./lib ./src -s",
    "postinstall": "npm run build",
    "watch:babel": "babel ./src --watch --out-dir ./lib",
    "commit": "git add . && git-cz",
    "flow": "flow"
  },
  "author": "David Alejandro LondoÃ±o",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.8.0",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-plugin-transform-flow-strip-types": "6.14.0",
    "babel-preset-es2015-node5": "1.2.0",
    "babel-preset-stage-3": "6.5.0",
    "debug": "2.*",
    "mongoose": ">=4.5",
    "rimraf": "2.5.4"
  },
  "devDependencies": {
    "arrow-mocha": "1.0.1",
    "babel-eslint": "6.0.4",
    "babel-plugin-transform-flow-strip-types": "6.14.0",
    "bluebird": "3.4.6",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "commitizen": "2.8.6",
    "cz-conventional-changelog": "1.2.0",
    "dirty-chai": "1.2.2",
    "eslint": "2.10.2",
    "eslint-config-airbnb-base": "3.0.1",
    "eslint-plugin-import": "1.8.0",
    "eslint-plugin-mocha": "4.5.1",
    "flow-bin": "^0.32.0",
    "mocha": "3.0.2",
    "sinon": "1.17.6",
    "sinon-as-promised": "4.0.2",
    "sinon-mongoose": "1.3.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
